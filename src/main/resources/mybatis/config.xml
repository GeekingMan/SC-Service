<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <typeAliases>
        <typeAlias alias="UUID" type="java.util.UUID"/>
        <typeAlias alias="UUIDTypeHandler" type="com.baiwangmaoyi.common.mybatis.UuidTypeHandler"/>
        <typeAlias alias="EnumTypeHandler" type="org.apache.ibatis.type.EnumTypeHandler"/>
        <package name="com.geek.sc.entity"/>
    </typeAliases>
    <typeHandlers>
        <typeHandler handler="com.baiwangmaoyi.common.mybatis.UuidTypeHandler"/>
    </typeHandlers>
    <plugins>
        <plugin interceptor="com.baiwangmaoyi.common.mybatis.TimestampInterceptor"/>
    </plugins>
</configuration>